               <herausgebernameLang>XSchule</herausgebernameLang>
               <herausgebernameKurz>XSchule</herausgebernameKurz>
            </codeliste>
            <versionCodeliste>
               <version>0.2</version>
               <beschreibung>Die Art der Bemerkung gibt an ob es sich um standardisierte Bemerkungen handelt, oder nicht. </beschreibung>
               <datumGueltigkeitAb>2022-02-22</datumGueltigkeitAb>
               <versionCodelistenHandbuch>1.2</versionCodelistenHandbuch>
            </versionCodeliste>
            <codelistenspalten>
               <code>
                  <spaltennameLang>Code</spaltennameLang>
                  <datentyp>string</datentyp>
                  <codeSpalte>true</codeSpalte>
                  <verwendung>required</verwendung>
                  <empfohleneCodeSpalte>true</empfohleneCodeSpalte>
               </code>
               <beschreibung>
                  <datentyp>string</datentyp>
                  <codeSpalte>false</codeSpalte>
