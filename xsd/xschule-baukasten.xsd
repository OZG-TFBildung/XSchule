   <xs:complexType name="Einschulung">
      <xs:annotation>
         <xs:documentation>Hier können Angaben zur erstmaligen Aufnahme eines Kindes in die Schullaufbahn gemacht werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="eintrittsdatum" minOccurs="0" type="xs:date"/>
         <xs:element name="artDerEinschulung"
                     minOccurs="0"
                     type="xsc:Code.ArtDerEinschulung"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Fremdsprache">
      <xs:annotation>
         <xs:documentation>Der Wert Fremdsprache fasst die Angaben zu den Sprachen die abgelegt wurden zusammen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="bezeichnung" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die Bezeichnung gibt Aufschluss über den Namen der Sprache, die abgelegt wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="beginn" type="xsc:Code.Klassenstufe">
            <xs:annotation>
               <xs:documentation>Die Wert Beginn gibt Aufschluss über den Anfang der Fremdsprache. In den Anwendungsfällen Schulwechsel und beim Ausstellen eines Abgangs- bzw. Abschlusszeugnisses besteht die Notwendigkeit eine Klassenstufe (1-13) angeben zu können.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ende" type="xsc:Code.Klassenstufe">
            <xs:annotation>
               <xs:documentation>Der Wert Ende gibt Aufschluss über das Ende der Belegung einer Fremdsprache. In den Anwendungsfällen Schulwechsel und beim Ausstellen eines Abgangs- bzw. Abschlusszeugnisses besteht die Notwendigkeit eine Klassenstufe (1-13) angeben zu können.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ger" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Für die modernen Fremdsprachen schließt dieses Zeugnis Kompetenzen des Gemeinsamen europäischen Referenzrahmens für Sprachen (GeR) ein.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Impfung">
      <xs:annotation>
         <xs:documentation>Hier können Angaben über den Impfschutz von Schüler:innen gemacht werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="impfstatus" type="xsc:Code.Impfstatus">
            <xs:annotation>
               <xs:documentation>Mit dem Impfstatus wird angegeben, ob ein Kind geimpft wurde oder nicht bzw. aus welchem Grund keine Angabe über die Impfung vorliegt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="krankheit" type="xsc:Code.Krankheit">
            <xs:annotation>
               <xs:documentation>Liste von Krankheiten, zu denen ein Impfstatus erhoben wird.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Nachrichtenkopf">
      <xs:annotation>
         <xs:documentation>Dieser Datentyp führt die technischen Inhalte zur Nachrichtenübermittlung im Schulkontext zusammen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="sender" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>In diesem Element wird der Sender des Nachweises übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="empfaenger" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>In diesem Element wird der Empfänger des Nachweises übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="erstellungszeitpunkt" type="xs:dateTime" use="required">
         <xs:annotation>
            <xs:documentation>In diesem Attribut wird der Erstellungszeitpunkt der Nachricht dokumentiert.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Pruefungsergebnis">
      <xs:annotation>
         <xs:documentation>Die Klasse Prüfungsergebnis fasst die Angaben zur den bei der Abiturprüfung abgelegten Prüfungen zusammen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="fach" type="xsc:Code.Faecherliste">
            <xs:annotation>
               <xs:documentation>Das Fach des Prüfungsergebnisses beschreibt den Namen des Faches in der die Abiturprüfung abgelegt wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="artDerPruefung" type="xbd:Code.ArtDerPruefung">
            <xs:annotation>
               <xs:documentation>Die Art der Prüfung kann verschiedene Ausprägungen haben. Bspw. kann angebenden werden, ob es sich um eine schriftlich oder mündlich abgelegte Prüfung handelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="note" type="xbd:Benotung">
            <xs:annotation>
               <xs:documentation>Die Benotungen der jeweiligen Prüfungen werden als ganzzahlige Werte abgebildet.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="nachpruefung" type="xsc:Code.Nachpruefung">
            <xs:annotation>
               <xs:documentation>Die Codeliste gibt an, ob es sich um eine Nachprüfung freiwilliger oder verpflichtender Art handelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Qualifikation">
      <xs:annotation>
         <xs:documentation>Die Klasse Qualifikation fasst die erreichte Gesamtpunktzahl und die ermittelte Durchschnittsnote zusammen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="gesamtpunktzahl" minOccurs="0" type="xbd:Benotung">
            <xs:annotation>
               <xs:documentation>Angaben zur erreichten Gesamtpunktzahl aus allen erbrachten Leistungen.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="durchschnittsnote" type="xbd:Benotung">
            <xs:annotation>
               <xs:documentation>Angaben zu der ermittelten Durchschnittsnote aus allen erbrachten Leistungen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Schueler">
      <xs:annotation>
         <xs:documentation>Der Schüler ist eine natürliche Person welche ein Bildungsangebot (Schule, Hochschule, Weiterbildungsstätte, Ausbildungsstätte, weitere) wahrgenommen hat und auf die ein Bildungsnachweis (z.B. Zeugnis) ausgestellt wurde.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Lernender">
            <xs:sequence>
               <xs:element name="kontaktmoeglichkeit"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xbd:Kommunikation">
                  <xs:annotation>
                     <xs:documentation>Die Kontaktmöglichkeit fasst Angaben zur Erreichbarkeit über elektronische Kommunikationskanäle (z.B. Telefon, Fax, E-Mail) zusammen.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="herkunftssprache"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xbd:Code.Language">
                  <xs:annotation>
                     <xs:documentation>Hier wird die Sprache angegeben, die eine Person als erstes erworben hat. In der Regel handelt es sich um die Sprache des Herkunftslandes.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="verkehrssprache"
                           maxOccurs="unbounded"
                           type="xbd:Code.Language">
                  <xs:annotation>
                     <xs:documentation>Hier wird die Sprache angegeben, die vorrangig im Haushalt des Kindes gesprochen wird.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="auskunftssperre" minOccurs="0" type="xbd:Code.Language">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die Tatsache des Vorliegens einer Auskunftssperre übermittelt. Liegt eine Auskunftssperre nach § 51 BMG mit einem Schlüssel 1, 3 oder 11 vor, ist das Element zu übermitteln (Die Sperren 6 und 12 führen nie zu der Übermittlung einer „Tatsache, dass eine Auskunftssperre vorliegt“). In diesem Element/Attribut ist nur Wert „true“ zulässig (fixed-Wert). Die Daten des Kindes dürfen nur beauskunftet werden, wenn für das Kind weder Auskunftssperre nach § 51 BMG noch ein bedingter Sperrvermerk nach § 52 BMG eingetragen ist (siehe § 18 Abs. 4 und § 18a Abs. 3 BMG).</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SchuelerReisender">
      <xs:annotation>
         <xs:documentation>Hier werden Angaben zu Schüler:innen gemacht, die sich aufgrund der beruflichen Situation der Sorgeberechtigten dauerhaft oder über lange Zeiträume auf Reisen befinden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="kindBeruflichReisender" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Angabe, ob ein Lernender in die Kategorie "Kind beruflich Reisender" fällt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="reiseMitUnternehmenName"
                     minOccurs="0"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann der Name des Unternehmens angegeben werden, mit welchem sich ein Kind auf Reisen befindet.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="reiseUnternehmenBranche"
                     minOccurs="0"
                     type="xsc:Code.BrancheDesUnternehmens">
            <xs:annotation>
               <xs:documentation>Angabe des Wirtschafts- oder Geschäftszweiges, dem das Unternehmen zugeordnet werden kann, mit dem sich das Kind auf Reisen befindet.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="stammschule" minOccurs="0" type="xsc:SchuleStammdaten">
            <xs:annotation>
               <xs:documentation>Hier können Angaben zu einer Schule gemacht werden, der ein Kind beruflich Reisender primär zugeordnet ist. Die Stammschule ist die Schule, an der die Kinder während der reisefreien Zeit über einen längeren Zeitraum verweilen. In der Regel ist dies eine Schule am Hauptwohnsitz oder am Winterstandort der beruflich reisenden Familie.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SchuleStammdaten">
      <xs:annotation>
         <xs:documentation>Die Klasse Schule enthält für Bildungseinrichtungen entlang der Lebenslage Schule, Hochschule, berufliche Aus- und Weiterbildung allgemeine Angaben zu Organisationen, die einen Bildungsnachweis ausstellen. Eurovoc-Term: http://publications.europa.eu/resource/authority/eurovoc/873</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Bildungseinrichtung">
            <xs:sequence>
               <xs:element name="erreichbarkeit"
                           maxOccurs="unbounded"
                           type="xbd:Kommunikation">
                  <xs:annotation>
                     <xs:documentation>Die Kontaktmöglichkeit fasst Angaben zur Erreichbarkeit über elektronische Kommunikationskanäle (z.B. Telefon, Fax, E-Mail) zusammen.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="aufnehmendeSchuleBekannt" minOccurs="0" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob aus Sicht eines Datensatzes einer abgebenden Schule bekannt ist, welches die aufnehmende Schule ist.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="schulform" minOccurs="0" type="xbd:Code.ArtDerSchule">
                  <xs:annotation>
                     <xs:documentation>Art der Bildungseinrichtung (Schulen) gemäß Kultusministerkonferenz vom 15.12.2011.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="schulprofil" minOccurs="0" type="xsc:Code.Schulprofil">
                  <xs:annotation>
                     <xs:documentation>Angabe der Festlegung einer Schule auf eine bestimmt Ausbildungsrichtung bzw. verschiedene Schulzweige. Sollte keine Angabe möglich sein, wird der Grund dafür angegeben.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="bildungsgang"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xsc:Code.Bildungsgang">
                  <xs:annotation>
                     <xs:documentation>Angabe des Bildungsganges an einer Schule. Das kann beispielsweise der G8 oder G9 Bildungsgang sein. Sollte keine Angabe möglich sein, wird der Grund dafür angegeben.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SchulformspezifischeZusatzdaten">
      <xs:annotation>
         <xs:documentation>Angaben zu schulformspezifischen Zusatzdaten.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="schulbesuchsjahreInSchuleingangsphase"
                     minOccurs="0"
                     type="xs:integer">
            <xs:annotation>
               <xs:documentation>Schulbesuchsjahre innerhalb der Schuleingangsphase</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="teilnahmeGemeinsamerUnterricht"
                     minOccurs="0"
                     type="xsc:Code.TeilnahmeGemeinsamerUnterricht"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Schuljahrgang">
      <xs:annotation>
         <xs:documentation>ier können Angaben zur Jahrgangsstufe eines Kindes gemacht werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="zeitraum" minOccurs="0" type="xbd:Zeitraum"/>
         <xs:element name="verweildauer" minOccurs="0" type="xs:integer"/>
         <xs:element name="versetzungstyp" minOccurs="0" type="xsc:Code.Versetzungstyp"/>
         <xs:element name="teilnahmeAmGanztagsangebot" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Hier kann angegeben werden, ob ein Kind an einem Ganztagesangebot teilgenommen hat oder nicht.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Schulkurs">
      <xs:annotation>
         <xs:documentation>Der Wert Leistung fasst die erbrachten Leistungen der Lernenden eines bestimmten Bildungsabschlusses zusammen.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Kurs">
            <xs:sequence>
               <xs:element name="bereich" minOccurs="0" type="dinspec91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Die Pflicht- und Wahlfächer umfassen - das sprachlich-literarisch-künstlerische Aufgabenfeld - das gesellschaftswissenschaftliche Aufgabenfeld - das mathematisch-naturwissenschaftlich-technische Aufgabenfeld - Religionslehre bzw. ein Ersatzfach - Sport. Für den Unterricht in Religionslehre bzw. im Ersatzfach gelten jeweils die Bestimmungen der Länder. Quelle: https://www.kmk.org/fileadmin/Dateien/veroeffentlichungen_beschluesse/1972/1972_07_07-VB-gymnasiale-Oberstufe-Abiturpruefung.pdf</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fach"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xsc:Code.Faecherliste">
                  <xs:annotation>
                     <xs:documentation>Der Schlüsselwert der Leistung.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="berechnungsgrundlage"
                           minOccurs="0"
                           type="dinspec91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Die Berechnungsgrundlage nach dem KMK Beschluss zur Umsetzung der Noten in Punkte gibt Aufschluss über die Umrechnung von Punkten, Noten und der Abbildung der Note in Wortschrift.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="kursart" type="xsc:Code.Kursart">
                  <xs:annotation>
                     <xs:documentation>Die Leistung kann in einer bestimmen Kursart erbracht werden. Die Kursarten werden zwischen Grund- und Leistungskurs unterschieden.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="bewertungsschemaAbitur"
                           minOccurs="0"
                           type="xbd:Code.Bewertungsschema">
                  <xs:annotation>
                     <xs:documentation>Die Berechnungsgrundlage nach dem KMK Beschluss zur Umsetzung der Noten in Punkte gibt Aufschluss über die Umrechnung von Punkten, Noten und der Abbildung der Note in Wortschrift.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="halbjahr" type="xsc:Code.Halbjahr">
                  <xs:annotation>
                     <xs:documentation>Der Schulkurs kann im ersten oder zweiten Halbjahr des Schuljahres stattfinden.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SorgeberechtigteOrganisation">
      <xs:annotation>
         <xs:documentation>Hier kann der Name einer Organisation angegeben werden, die für das Kind sorgeberechtigt ist. Dies kann beispielsweise das Jugendamt sein.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Sorgeberechtigter">
            <xs:sequence>
               <xs:element name="NameSorgeberechigteOrganisation"
                           minOccurs="0"
                           type="dinspec91379:datatypeC"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Sorgeberechtigter">
      <xs:annotation>
         <xs:documentation>Hier kann der Name einer Organisation angegeben werden, die für das Kind sorgeberechtigt ist. Dies kann beispielsweise das Jugendamt sein.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Sorgeberechtigter">
            <xs:sequence>
               <xs:element name="anschriftZuzug" minOccurs="0" type="xbd:Meldeanschrift">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Datentyp wird die Inlandsanschrift einer Person abgebildet. Er basiert auf der Anschrift des XInneres-Basismoduls.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="beziehungZumSchueler"
                           minOccurs="0"
                           type="xbd:Code.ArtDesSorgeberechtigten">
                  <xs:annotation>
                     <xs:documentation>Hier muss eine Angabe gemacht werden, in welchem Verhältnis die sorgeberechtigte Person zum Kind steht. Sollte keine Angabe gemacht werden können, wird der Grund dafür angegeben.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="kontaktmoeglichkeit"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xbd:Kommunikation">
                  <xs:annotation>
                     <xs:documentation>Die Kontaktmöglichkeit fasst Angaben zur Erreichbarkeit über elektronische Kommunikationskanäle (z.B. Telefon, Fax, E-Mail) zusammen.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="auskunftssperre" minOccurs="0" type="xbd:Code.Language">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die Tatsache des Vorliegens einer Auskunftssperre übermittelt. Liegt eine Auskunftssperre nach § 51 BMG mit einem Schlüssel 1, 3 oder 11 vor, ist das Element zu übermitteln (Die Sperren 6 und 12 führen nie zu der Übermittlung einer „Tatsache, dass eine Auskunftssperre vorliegt“). In diesem Element/Attribut ist nur Wert „true“ zulässig (fixed-Wert). Die Daten des Kindes dürfen nur beauskunftet werden, wenn für das Kind weder Auskunftssperre nach § 51 BMG noch ein bedingter Sperrvermerk nach § 52 BMG eingetragen ist (siehe § 18 Abs. 4 und § 18a Abs. 3 BMG).</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Unterrichtsteilnahme">
      <xs:annotation>
         <xs:documentation>Es können Angaben zu einer bestimmten Unterrichtsteilnahme gemacht werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="fach" minOccurs="0" type="xsc:Code.Faecherliste">
            <xs:annotation>
               <xs:documentation>Der Name des Faches, an dem teilgenommen wurde als ausgeschriebenes Wort.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="arbeitsgemeinschaft"
                     minOccurs="0"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Name der Arbeitsgemeinschaft, an der teilgenommen wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="unterrichtsteilnahmeBezeichnung"
                     minOccurs="0"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann eine Bemerkung zu Freistellung, Wahl-, Pflichtteilnahme erfolgen.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="unterrichtsteilnahme"
                     minOccurs="0"
                     type="xsc:Code.Unterrichtsteilnahme">
            <xs:annotation>
               <xs:documentation>Hier kann eine Bemerkung zu Freistellung, Wahl-, Pflichtteilnahme erfolgen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Zeugnis">
      <xs:annotation>
         <xs:documentation>Mit dem Bildungsnachweis wird ein Bildungsabschluss nachgewiesen.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xbd:Bildungsnachweis">
